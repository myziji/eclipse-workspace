package blind75;

import java.util.Stack;

public class MinStack {
	
	public static void main(String[] args) {
		
	}
	
    public MinStack() {
    	Stack<Integer> minStack = new Stack<>();
    	Stack<Integer> stack = new Stack<>();
    }
    
    public void push(int val) {
    
    	stack.push(val);
    	if(minStack.isEmpty()) {
    		minStack.push(val);
    	}else {
    		if(val>=minStack.peek()) {
    			minStack.push(val);
    		}else {
    			minStack.push(top());
    		}
    	}
        
    }
    
    public void pop() {
        
    }
    
    public int top() {
        
    }
    
    public int getMin() {
        
    }
}
