package blind75;

public class ContainsDuplicate {

	
	public static void main(String agrs[]) {
		int[] arr = {1,2,3,4,5,1};
		System.out.println(containsDuplicate(arr));
		//solution 1
		
	}
	
	
	public static boolean containsDuplicate(int[] nums) {
		for(int i=0;i<nums.length;i++) {
			for(int j=0; j<i;j++) {
				if(nums[i]==nums[j]) {
					return true;
				}
			}
		}
		return false;
	}
	
}
